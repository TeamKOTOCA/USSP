{
  "name": "@ussp/sdk",
  "version": "1.0.0",
  "description": "JavaScript SDK for USSP - Distributed Storage Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.json && node scripts/build-cjs.js",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build && npm run test"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "keywords": [
    "storage",
    "distributed",
    "sdk",
    "ussp",
    "oauth",
    "backup"
  ],
  "author": "USSP Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TeamKOTOCA/USSP"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
